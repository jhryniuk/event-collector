            <ul class="nav navbar-nav">
                {#<li><a href="{{ path('dashboard_homepage') }}">Dashboard</a></li>#}
                {#<li><a href="{{ path('gallery_index') }}">Galeria</a></li>#}
                {#<li><a href="{{ path('documents_index') }}">Dokumenty</a></li>#}
                {#<li><a href="{{ path('settings_index') }}">Ustawienia</a></li>#}
                <li><a href="{{ path('logout') }}">Wyloguj</a></li>
            </ul>
        </nav>
    {% endif %}
    {% block body %}{% endblock %}
</div>
{% block javascripts %}
